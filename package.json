{
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format esm,cjs",
    "lint": "tsdx lint",
    "prettier": "prettier --write ./src",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 9000",
    "build-storybook": "build-storybook",
    "ssr-test": "node ssr-test.js",
    "test": "test-it --test-setup-files ./test-setup.ts && yarn ssr-test",
    "test:watch": "test-it --test-setup-files ./test-setup.ts --watch"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write ./src && tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "name": "react-laag",
  "author": "Erik Verweij",
  "module": "dist/react-laag.esm.js",
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@test-it/cli": "^0.6.4",
    "@test-it/core": "^0.6.6",
    "@testing-library/react": "^11.0.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.0",
    "babel-loader": "^8.1.0",
    "downshift": "^5.4.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.3",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-is": "^16.13.0",
    "rollup-plugin-filesize": "^9.0.0",
    "styled-components": "^5.1.1",
    "ts-loader": "^8.0.4",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "tiny-warning": "^1.0.3"
  }
}
